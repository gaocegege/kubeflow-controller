{
  "Vendor": true,
  "DisableAll": true,
  "Enable": [
    "vet",
    "safesql",
    "goconst",
    "goimports",
    "golint",
    "gas",
    "lll",
    "megacheck",
    "unconvert",
    "misspell"
  ],
  "LineLength": 240,
  "Deadline": "300s",
  "Aggregate": true,
  "WarnUnmatchedNolint": true,
  "Exclude": ["comment or be unexported", "comment on exported"]
}
